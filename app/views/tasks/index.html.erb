<h1>タスク一覧</h1>
<table>
  <thead>
    <tr>
      <th>タスク名</th>
    </tr>
  </thead>
  <tbody>
    <% @tasks.each do |t| %>
      <tr>
        <td><%= t.title %></td>
        <td><%= link_to '編集',edit_task_path(t) %></td>
        <td><%= link_to '削除',task_path(t), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to '追加',new_task_path %>